                        <h5 class="modal-title" id="modalTareaTitle">Nueva tarea</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        
                        <div class="form-group">
                            <div class="row">
                                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                    <label for="tareaTitle"
                                        ng-class="{ 
                                            'col-form-label': vm.formTarea.tareaTitle.$pristine || vm.formTarea.tareaTitle.$valid, 
                                            'col-form-label text-danger': !vm.formTarea.tareaTitle.$pristine && !vm.formTarea.tareaTitle.$valid }">Título (requerido)</label> 
                                    <input id="tareaTitle" name="tareaTitle" placeholder="Mínimo 3 caracteres" type="text" 
                                        ng-class="{ 
                                        'form-control': vm.formTarea.tareaTitle.$pristine || vm.formTarea.tareaTitle.$valid, 
                                        'form-control is-invalid': !vm.formTarea.tareaTitle.$pristine && !vm.formTarea.tareaTitle.$valid }" 
                                        ng-maxlength="130" 
                                        ng-minlength="3" 
                                        ng-model="vm.tareaTitle" 
                                        ng-required="true" /> 
                                    <p class="text-danger" role="alert" ng-messages="!vm.formTarea.tareaTitle.$pristine && vm.formTarea.tareaTitle.$error"> 
                                        <strong ng-message="minlength">Mínimo 3 caracteres</strong> 
                                        <strong ng-message="maxlength">Nombre muy largo</strong> 
                                        <strong ng-message="required">Requerido</strong> 
                                    </p> 
                                </div> 
                            </div> 
                        </div> 
                        
                        <div class="form-group"> 
                            <div class="row"> 
                                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12"> 
                                    <label for="tareaUser">Responsable</label> 
                                    <select id="tareaUser" name="tareaUser" style="width: 100%"> 
                                    </select> 
                                </div> 
                            </div>
                        </div>

                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-primary" type="button" data-dismiss="modal"><span class="glyphicon glyphicon-remove-circle"></span> Cerrar</button>
                        <button type="button"
                            ng-class="{ 
                                'btn btn-success': vm.formTarea.$valid, 
                                'btn btn-default': !vm.formTarea.$valid }" 
                            ng-click="vm.onBtnTareaGuardarClick()"
                            ng-disabled="!vm.formTarea.$valid">
                            <span class="glyphicon glyphicon-save"></span> Guardar
                        </button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!-- Fin modal Tarea -->

</div>